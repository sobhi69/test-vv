{
    "version": 2,
    "buildCommand":"tsc",
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dest/**"] }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.ts",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }